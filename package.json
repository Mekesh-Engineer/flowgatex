{
  "name": "flowgatex",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:html": "node scripts/lint-html-no-inline.mjs index.html",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "analyze": "vite-bundle-visualizer",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "clean": "node cleanup.cjs",
    "clean:all": "node cleanup.cjs --all",
    "clean:cache": "node cleanup.cjs --cache",
    "docker:build": "docker build -t flowgatex:latest .",
    "docker:run": "docker run -d -p 80:80 --name flowgatex flowgatex:latest",
    "docker:stop": "docker stop flowgatex",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@react-google-maps/api": "^2.20.8",
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.5",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "firebase": "^12.9.0",
    "framer-motion": "^11.18.2",
    "fuse.js": "^7.1.0",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "ics": "^3.8.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.564.0",
    "qrcode.react": "^4.2.0",
    "razorpay": "^2.9.6",
    "react": "^19.2.4",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.13.0",
    "sweetalert2": "^11.26.18",
    "tailwind-merge": "^3.4.1",
    "vercel": "^50.19.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/resolve": "^1.20.6",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lightningcss": "^1.31.1",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.10",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.css": [
      "prettier --write"
    ]
  }
}
